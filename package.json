{
  "name": "Aga-Language",
  "displayName": "Lenguaje Aga",
  "description": "suporte para el Lenguaje Aga",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "icon": "aga.png",
  "publisher": "aga-language",
  "categories": [
    "Programming Languages"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [{
      "id": "agalanguage",
      "aliases": ["Lenguaje Aga", "agalanguage"],
      "extensions": [".agal"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "agalanguage",
      "scopeName": "source.agal",
      "path": "./syntaxes/agalanguage.tmLanguage.json"
    }],
    "snippets": [{
      "language": "agalanguage",
      "path": "./snippets/agalanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AgaDevInc/AgaL-vscode.git",
    "branch": "main"
  },
  "license": "MIT"
}
